[
    {
        "src": "/api/organization/(?<username>[^/]+)",
        "methods": ["GET"],
        "dest": "/api/organization/get?pathIds.username=$username"
    },
    {
        "src": "/api/organization/(?<username>[^/]+)/repository/(?<repositoryName>[^/]+)",
        "methods": ["GET"],
        "dest": "/api/organization/repository/get?pathIds.username=$username&pathIds.repositoryName=$repositoryName"
    },
    {
        "src": "/api/organization/(?<username>[^/]+)/languages",
        "methods": ["GET"],
        "dest": "/api/organization/languages/get?pathIds.username=$username"
    }
]